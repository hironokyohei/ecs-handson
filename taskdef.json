{
    "family": "console-sample-app",
    "networkMode": "awsvpc",
    "cpu": "512",
    "executionRoleArn": "arn:aws:iam::113857159340:role/ecsTaskExecutionRole",
    "memory": "1024",
    "volumes": [
        {
            "name": "my-vol",
            "host": {}
        }
    ],
    "containerDefinitions": [
        {
            "environment": [],
            "name": "simple-app",
            "image": "<IMAGE1_NAME>",
            "cpu": 256,
            "memory": 512,
            "portMappings": [
                {
                    "containerPort": 80
                }
            ],
            "mountPoints": [
                {
                    "sourceVolume": "my-vol",
                    "containerPath": "/var/www/my-vol"
                }
            ],
            "essential": true
        }
    ]
}
